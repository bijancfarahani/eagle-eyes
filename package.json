      "copy-webpack-plugin": "12.0.2",
      "eslint": "^9.22.0",
      "eslint-config-prettier": "^10.1.1",
      "eslint-plugin-prettier": "^5.2.3",
      "expose-loader": "5.0.0",
      "gh-pages": "^6.3.0",
      "globals": "^16.0.0",
      "html-webpack-plugin": "5.6.0",
      "prettier": "^3.5.3",
